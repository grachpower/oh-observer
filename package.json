    "url": "https://github.com/grachpower/oh-observer/issues"
  },
  "homepage": "https://github.com/grachpower/oh-observer#readme",
  "dependencies": {
    "express": "^4.16.3",
    "jest": "^23.1.0",
    "nodemon": "^1.17.5",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.2"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "rimraf": "^2.6.2",
    "size-limit": "^0.18.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "size-limit": [
    {
      "path": "lib/*.js",
      "limit": "9 KB"
    }
  ]
